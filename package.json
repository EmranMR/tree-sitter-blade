{
  "name": "tree-sitter-blade",
  "version": "0.11.0",
  "homepage": "https://github.com/EmranMR/tree-sitter-blade",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmranMR/tree-sitter-blade"
  },
  "bugs": "https://github.com/EmranMR/tree-sitter-blade/issues",
  "funding": "https://github.com/sponsors/EmranMR",
  "license": "MIT",
  "description": "tree-sitter-blade grammar for Laravel Blade files",
  "author": {
    "name": "Emran Mashhadi Ramezan",
    "email": "2t5ukanu@duck.com"
  },
  "contributors": {
    "name": "Caleb White",
    "email": "cdwhite3@pm.me"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter-blade",
    "blade",
    "tree-sitter",
    "laravel"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.2",
    "node-gyp-build": "^4.8.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.2",
    "@types/node": "^22.13.0"
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint grammar.js",
    "build": "deno -A ./main/bundle.ts && tree-sitter generate",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
